<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@+id/home_divider_first0"
        android:layout_width="match_parent"
        android:layout_height="@dimen/divider_height"
        android:layout_marginStart="@dimen/home_divider_marginside"
        android:layout_marginEnd="@dimen/home_divider_marginside"
        android:background="@drawable/shape_divider"
        android:backgroundTint="@color/divider_up"
        android:visibility="visible" />
    <View
        android:layout_marginTop="@dimen/divider2_marginTop"
        android:id="@+id/home_divider_first1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/divider_height"
        android:layout_marginStart="@dimen/home_divider_marginside2"
        android:layout_marginEnd="@dimen/home_divider_marginside2"
        android:background="@drawable/shape_divider"
        android:backgroundTint="@color/lowblue"
        android:visibility="visible" />

</LinearLayout>